{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.gv.hackathon.Hackathon3",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize",
			"sap_belize_plus"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.gv.hackathon.Hackathon3.view.Authorization",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.gv.hackathon.Hackathon3.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.gv.hackathon.Hackathon3.view",
				"controlAggregation": "app",
				"controlId": "shell",
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "",
				"name": "home",
				"target": "homeApp"
			}, {
				"pattern": "0",
				"name": "workorders",
				"target": "WorkOrdersView"
			}],
			"targets": {
				"homeApp": {
					"viewName": "Authorization",
					"viewLevel": 1
				},
				"WorkOrdersView": {
					"viewName": "WorkOrders"
				}
			}
		}
	}
}